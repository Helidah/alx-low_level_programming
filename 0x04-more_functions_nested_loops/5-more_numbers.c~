#include "holberton.h"

/**
 * void_more_numbers - Prints the number zero to nine repeatedly
 *
 * Return: Always 0 (Success)
 */

void more_numbers(void)
{
int i;
int j;

for (i = 0; i < 10; i++)
{
for (j = 0; j < 15; j++)
{
if (j < 10 && j > 0)
{
_putchar(j + '0');
}
else if (j > 9)
{
_putchar((j) / 10 + '0');
_putchar((j) % 10 + '0');
}
}

_putchar('\n');
}

}
